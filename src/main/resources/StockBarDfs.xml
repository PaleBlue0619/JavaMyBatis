<mapper namespace="com.maxim.demo.dao.queryStockBar">
    <select id="queryByTimeSpan" resultType="com.maxim.demo.entity.KBarEntity">
            <![CDATA[
               select *
               from o_tushare_a_stock_daily
               where trade_date >= #{start_date} and trade_date <= #{end_date}
            ]]>
    </select>
</mapper>